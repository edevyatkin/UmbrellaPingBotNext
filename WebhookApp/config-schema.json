{
  "type": "object",
  "properties": {
    "token": {
      "type": "string",
      "pattern": "^[0-9]{9}:[-_a-zA-Z0-9]{0,35}$"
    },
    "bot": {
      "type": "string"
    },
    "sw_info_bot": {
      "type": "string"
    },
    "chats": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "proxy": {
      "type": "object",
      "properties": {
        "server": {
          "type": "string"
        },
        "port": {
          "type": "integer"
        },
        "login": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      },
      "required": [
        "server",
        "port",
        "login",
        "password"
      ]
    },
    "usernames": {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "lottery_usernames": {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "webhook_url": {
      "type": "string"
    },
    "chat_admins": {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    }
  },
  "required": [
    "chats",
    "chat_admins"
  ]
}